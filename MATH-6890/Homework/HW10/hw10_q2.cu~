#include <stdio.h>

typedef double Real;
__global__ void add( Real *a_d, Real *b_d, Real *c_d, int n, int Nt ){
	   int index = threadIdx.x + blockIdx.x*Nt;
	   if (index < n){
	      a_d[index] = -index;
	      b_d[index] = index*index;
	      c_d[index] = a_d[index] + b_d[index];
	   }
}

int main( int argc, char *argv[] ) {

    
    return 0;
}